{
  "name": "backend-nest",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand",
    "seed:grc": "ts-node -r tsconfig-paths/register src/scripts/seed-grc.ts",
    "seed:grc:prod": "node dist/scripts/seed-grc.js",
    "seed:onboarding": "ts-node -r tsconfig-paths/register src/scripts/seed-onboarding.ts",
    "seed:onboarding:prod": "node dist/scripts/seed-onboarding.js",
    "seed:standards": "ts-node -r tsconfig-paths/register src/scripts/seed-standards.ts",
    "seed:standards:prod": "node dist/scripts/seed-standards.js",
    "seed:frameworks": "ts-node -r tsconfig-paths/register src/scripts/seed-frameworks.ts",
    "seed:frameworks:prod": "node dist/scripts/seed-frameworks.js",
    "seed:golden-flow": "ts-node -r tsconfig-paths/register src/scripts/seed-golden-flow.ts",
    "seed:golden-flow:prod": "node dist/scripts/seed-golden-flow.js",
    "seed:demo-story": "ts-node -r tsconfig-paths/register src/scripts/seed-demo-story.ts",
    "seed:demo-story:prod": "node dist/scripts/seed-demo-story.js",
    "smoke:grc": "ts-node -r tsconfig-paths/register src/scripts/smoke-grc.ts",
    "smoke:golden-flow": "ts-node -r tsconfig-paths/register src/scripts/smoke-golden-flow.ts",
    "smoke:golden-flow:prod": "node dist/scripts/smoke-golden-flow.js",
    "acceptance:full": "ts-node -r tsconfig-paths/register src/scripts/acceptance-runner.ts",
    "migration:generate": "typeorm migration:generate -d src/data-source.ts",
    "migration:status": "ts-node -r tsconfig-paths/register src/scripts/migration-status.ts",
    "migration:status:prod": "node dist/scripts/migration-status.js",
    "migration:run": "ts-node -r tsconfig-paths/register src/scripts/migration-run.ts",
    "migration:run:prod": "node dist/scripts/migration-run.js",
    "migration:revert": "typeorm migration:revert -d src/data-source.ts",
    "migration:show": "typeorm migration:show -d src/data-source.ts",
    "migration:show:prod": "typeorm migration:show -d dist/data-source.js",
    "migrations:status:prod": "node dist/scripts/migrations-status.js",
    "validate:env": "ts-node -r tsconfig-paths/register src/scripts/validate-env.ts",
    "validate:env:prod": "node dist/scripts/validate-env.js",
    "validate:db": "ts-node -r tsconfig-paths/register src/scripts/validate-db.ts",
    "validate:db:prod": "node dist/scripts/validate-db.js",
    "validate:migrations": "ts-node -r tsconfig-paths/register src/scripts/validate-migrations.ts",
    "validate:migrations:prod": "node dist/scripts/validate-migrations.js",
    "schema:contract": "ts-node -r tsconfig-paths/register src/scripts/schema-contract.cli.ts",
    "schema:contract:prod": "node dist/scripts/schema-contract.cli.js",
    "smoke:auth-onboarding": "ts-node -r tsconfig-paths/register src/scripts/smoke-auth-onboarding.ts",
    "smoke:auth-onboarding:prod": "node dist/scripts/smoke-auth-onboarding.js",
    "smoke:security": "ts-node -r tsconfig-paths/register src/scripts/smoke-security.ts",
    "smoke:security:prod": "node dist/scripts/smoke-security.js",
    "platform:validate": "ts-node -r tsconfig-paths/register src/scripts/platform-validate.ts",
    "platform:validate:prod": "node dist/scripts/platform-validate.js",
    "db:test:bootstrap": "ts-node -r tsconfig-paths/register src/scripts/db-test-bootstrap.ts"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.10",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.10",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.10",
    "@nestjs/swagger": "^11.2.4",
    "@nestjs/throttler": "^6.5.0",
    "@nestjs/typeorm": "^11.0.0",
    "@types/bcrypt": "^6.0.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "dotenv": "^17.2.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.28"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.39.2",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.1.10",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.7",
    "@types/bcryptjs": "^3.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.19.5",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.2.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.50.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
